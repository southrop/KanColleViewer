			<Setter.Value>
				<ControlTemplate TargetType="{x:Type controls:NavigationButton}">
					<Grid>
						<Ellipse StrokeThickness="{TemplateBinding BorderThickness}"
								 Stroke="{TemplateBinding BorderBrush}"
								 Fill="{TemplateBinding Background}" />
